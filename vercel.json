{
      "version": 2,
      "builds": [
          {
              "src": "./index.js",
              "use": "@vercel/node"
          }
      ],
      "routes": [
          {
              "src": "/(.*)",
              "dest": "/index.html",
              "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
          }
      ]
  }

